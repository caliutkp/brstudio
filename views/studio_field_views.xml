<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record id="view_studio_field_form" model="ir.ui.view">
        <field name="name">studio.field.form</field>
        <field name="model">studio.field</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <field name="name"/>
                        <field name="model_id"/>
                        <field name="field_type"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <record id="view_studio_field_tree" model="ir.ui.view">
        <field name="name">studio.field.tree</field>
        <field name="model">studio.field</field>
        <field name="arch" type="xml">
            <tree>
                <field name="name"/>
                <field name="model_id"/>
                <field name="field_type"/>
            </tree>
        </field>
    </record>
</odoo>